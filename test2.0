-- Set your webhook, webhook delay, and boxtrain settings
getgenv().boxtrain = "false"  -- Set to "true" if you want to enable box training

-- Load the first script
loadstring(game:HttpGet("https://raw.githubusercontent.com/VarsityB/meow/main/joe", true))()

-- Load the script to destroy the map
local success, err = pcall(loadstring(game:HttpGet("https://raw.githubusercontent.com/VarsityB/Autofarm/main/destroy%20map")))
if not success then
    warn("Error loading destroy map script:", err)
end

-- Load the premium GUI script
loadstring(game:HttpGet('https://raw.githubusercontent.com/VarsityB/Autofarm/main/premium%20gui'))()

-- Wait for the first scripts to run
wait(5)  -- Adjust the wait time based on your needs

-- Check if boxtrain is true and load the hubba script
if getgenv().boxtrain == "true" then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/VarsityB/Autofarm/main/hubba"))()

    -- Additional modifications
    local targetPosition = Vector3.new(-277.3309020996094, 23.595672607421875, -1125.0675048828125)

    -- Wait for the player to teleport
    wait(2)

    -- Find the item in the shop
    local itemName = "Boxing Moveset (Require: Max Box Stat) - $0"
    local itemToBuy = workspace.Ignored.Shop:FindFirstChild(itemName)

    if itemToBuy then
        -- Click the item's ClickDetector
        fireclickdetector(itemToBuy.ClickDetector)
    else
        warn(itemName .. " not found in the shop.")
    end
else
    print("Box training is disabled.")
end

-- Execute the rest of the script for auto-boxing farm only if boxtrain is true
if getgenv().boxtrain == "true" then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/VarsityB/Autofarm/main/boxing%20farm"))()
else
    print("Auto-boxing farm is disabled.")
end
