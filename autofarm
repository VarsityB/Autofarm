loadstring(game:HttpGet("https://raw.githubusercontent.com/VarsityB/Autofarm/main/destory%20map"))()

loadstring(game:HttpGet('https://raw.githubusercontent.com/VarsityB/Autofarm/main/premium%20gui'))()

-- Start with Enabled set to false
getgenv().Enabled = false

getgenv().Dis = function()
    Enabled = false

    for i, v in pairs(workspace:GetDescendants()) do
        if (v:IsA("Seat")) or (v:IsA("VehicleSeat")) then
            v.Disabled = true
        end
    end
end

if (not game:IsLoaded()) then
    game.Loaded:Wait()
    task.wait(2)
end

repeat
    task.wait(0.2)
until (game:GetService("Players").LocalPlayer) and (game:GetService("Players").LocalPlayer.Character)

local Players = game:GetService("Players")
local Cashiers = workspace.Cashiers
local Player = Players.LocalPlayer

-- Load the first script
local success, result = pcall(function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/VarsityB/Autofarm/main/boxing%20farm"))()
end)

if not success then
    warn("Failed to load the first script:", result)
    return
end

local startTime = tick() -- Record the start time

-- Wait for 10 seconds
repeat
    task.wait(1)

    -- Continue punching or any other action you want
    if (Enabled == true) and (Player.Character) and (Player.Character:FindFirstChild("FULLY_LOADED_CHAR")) then
        Player.Character.Combat:Activate()
        -- You may add more actions here if needed
    end
until tick() - startTime >= 10

-- Unequip Combat tool
if Player.Character and Player.Character:FindFirstChild("Combat") then
    Player.Character.Combat:Destroy()
    print("Unequipped Combat tool.")
end

-- Load and execute the second script
local secondScript = loadstring(game:HttpGet("https://raw.githubusercontent.com/VarsityB/Autofarm/main/autofarm"))
if secondScript then
    secondScript()
    print("Second script executed.")
else
    warn("Failed to load and execute the second script.")
end
